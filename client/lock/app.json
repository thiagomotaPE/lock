{
  "expo": {
    "name": "Lock",
    "slug": "lock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon-white.png",
    "scheme": "lock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2F13DE",
        "foregroundImage": "./src/assets/images/icon-white.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.lock"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/icon-blue.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-shadow.png",
          "imageWidth": 125,
          "resizeMode": "contain",
          "backgroundColor": "#2F13DE"
        }
      ],
      "expo-font",
      [
        "expo-font",
          {
            "fonts": ["node_modules/@expo-google-fonts/poppins/900Black/poppins_900Black.ttf"]
          }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
